{
  "name": "@conte-ltd/prisma-zod-generator",
  "version": "0.0.13",
  "description": "Prisma 2+ generator to emit Zod schemas from your Prisma schema",
  "repository": "https://github.com/conte-ltd/prisma-zod-generator",
  "bin": {
    "prisma-zod-generator": "lib/generator.js"
  },
  "scripts": {
    "build": "tsc",
    "generate": "tsc && npx prisma format && npx prisma generate",
    "package:publish": "./package.sh && cd package && npm publish"
  },
  "author": {
    "name": "CONTE, Ltd.",
    "url": "https://github.com/conte-ltd"
  },
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-schema",
    "zod",
    "prisma-generator",
    "prisma-zod-generator"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@prisma/generator-helper": "^4.12.0",
    "@prisma/internals": "^4.12.0",
    "prettier": "^2.8.7",
    "ts-morph": "^18.0.0",
    "tslib": "^2.5.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "bugs": {
    "url": "https://github.com/conte-ltd/prisma-zod-generator/issues"
  },
  "homepage": "https://github.com/conte-ltd/prisma-zod-generator#readme",
  "publishConfig": {
    "access": "public"
  }
}
